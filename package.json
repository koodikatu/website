{
	"name": "website",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit dev --host",
		"build": "svelte-kit build",
		"build:preview": "PREVIEW=true svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "jest",
		"test:watch": "npm run test -- --watch",
		"test:update": "npm run test -- --updateSnapshot",
		"dependencies:check": "ncu",
		"dependencies:update": "ncu -u"
	},
	"devDependencies": {
		"@babel/core": "7.17.10",
		"@babel/preset-env": "7.17.10",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "1.0.0-next.56",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "0.5.0",
		"@tailwindcss/typography": "0.5.2",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.1",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^27.4.1",
		"@types/testing-library__jest-dom": "^5.14.3",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"autoprefixer": "10.4.5",
		"babel-jest": "28.0.3",
		"cssnano": "5.1.7",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"jest": "^28.0.3",
		"jest-svg-transformer": "1.0.0",
		"jest-transform-stub": "2.0.0",
		"npm-check-updates": "12.5.10",
		"postcss": "8.4.13",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.0",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "4.10.6",
		"tailwindcss": "3.0.24",
		"ts-jest": "^27.1.4",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.8",
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.5.0",
		"dotenv": "16.0.0",
		"fp-ts": "2.12.1",
		"io-ts": "2.2.16"
	}
}
